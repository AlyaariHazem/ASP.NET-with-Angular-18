{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "-moz-box-align",
          "-moz-box-pack",
          "-webkit-overflow-scrolling",
          "min-height: auto",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "css-prefix-order": "off",
    "typescript-config/consistent-casing": "off",
    "button-type": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ]
  }
}